{
  "name": "heightify",
  "version": "4.0.0",
  "description": "A module for setting the same height on given elements",
  "main": "./lib/heightify.js",
  "scripts": {
    "clean": "rimraf lib",
    "test": "cross-env BABEL_ENV=commonjs mocha --compilers js:babel-register --recursive --require test/testhelper.js test/**/*.test.js",
    "test:watch": "npm test -- --watch",
    "examples": "browserify examples/index.js -o examples/heightify.example.js",
    "build": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "prepublish": "npm run clean && npm run test && npm run build",
    "dev": "npm run build && npm run examples"
  },
  "pre-commit": [
    "build",
    "test"
  ],
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oyvindhermansen/heightify.git"
  },
  "keywords": [
    "npm",
    "module",
    "node",
    "javascript",
    "heightify",
    "equal height",
    "ES6",
    "ES2015"
  ],
  "author": "Ã˜yvind Hermansen <oyvind@ludensreklame.no>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oyvindhermansen/heightify/issues"
  },
  "homepage": "https://github.com/oyvindhermansen/heightify#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.11.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "istanbul": "^1.0.0-alpha",
    "jsdom": "^9.4.2",
    "mocha": "^3.0.0",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "imagesloaded": "^4.1.0",
    "pre-commit": "^1.1.3"
  }
}
